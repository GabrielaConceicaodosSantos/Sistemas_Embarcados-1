#include "Display.h"
#include "Botao.h"
using namespace std;

int main() {

	struct Decisao decisao;
	decisao.GPIO[0] = 13;
	decisao.GPIO[1] = 19;
	decisao.GPIO[2] = 26;
	Configurar_Botoes(&decisao);




	GPIO_free(decisao.GPIO[0]);
	GPIO_free(decisao.GPIO[1]);
	GPIO_free(decisao.GPIO[2]);
	return 0;
}


